<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<bean name="TB_BOARD_MANAGE_SEQ" class="egovframework.rte.fdl.idgnr.impl.EgovSequenceIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="query" value="SELECT SEQ_NAME_NEXTVAL('TB_BOARD_MANAGE') FROM DUAL"/>
	</bean>
	
	<bean name="TB_HASHTAG_SEQ" class="egovframework.rte.fdl.idgnr.impl.EgovSequenceIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="query" value="SELECT SEQ_NAME_NEXTVAL('TB_HASHTAG') FROM DUAL"/>
	</bean>
	
	<bean name="TB_INTEREST_SEQ" class="egovframework.rte.fdl.idgnr.impl.EgovSequenceIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="query" value="SELECT SEQ_NAME_NEXTVAL('TB_INTEREST') FROM DUAL"/>
	</bean>

</beans>